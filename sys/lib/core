DerpyOS_System_Version = "004"

function DerpyOS_System_GetName()
  shell.run("/sys/etc/config")
  return DerpyOS_Config_SystemName
end

function DerpyOS_System_Shutdown()
  print("System "..DerpyOS_System_GetName().." going down at "..os.time())
  os.shutdown()
end

function DerpyOS_System_Reboot()
  print("System "..DerpyOS_System_GetName().." rebooting at "..os.time())
  os.reboot()
end
